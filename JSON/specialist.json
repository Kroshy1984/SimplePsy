{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Психолог",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "description": "идентификатор психолог"
    },
    "name": {
      "type": "string",
      "description": "Имя",
      "maxLength": 255,
      "pattern": "^[А-я]+*[а-я]$"
    },
    "surname": {
      "type": "string",
      "description": "Фамилия",
      "maxLength": 255,
      "pattern": "^[А-Я]+*[а-я]$"
    },
    "middleName": {
      "type": "string",
      "description": "Отчество",
      "maxLength": 255,
      "pattern": "^[А-Я]+*[а-я]$"
    },
    "sessions": {
      "$ref": "#/definitions/session_type"
    },
    "notification": {
      "$ref": "#/definitions/session_type"
    }
  },
  "required": [
    "name",
    "surname",
    "id"
  ]
}
